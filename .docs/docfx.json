{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../../Prefrontal",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "output": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "files/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
	  "modern",
      "template"
    ],
    "globalMetadata": {
      "_appName": "Prefrontal",
      "_appTitle": "Prefrontal",
	  "_appLogoPath": "files/logo.svg",
	  "_appFaviconPath": "files/favicon.ico",
      "_enableSearch": true,
	  "_disableContribution": true,
      "pdf": false
    }
  }
}
